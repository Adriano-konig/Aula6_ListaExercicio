programa {
  funcao inicio() {
    inteiro primeiroNumero
    inteiro segundoNumero

    escreva("Digite primeiro numero: ")
    leia(primeiroNumero)
    escreva("Digite segundo numero: ")
    leia(segundoNumero)

    inteiro soma
    soma = primeiroNumero + segundoNumero

    escreva("Resultado da soma é: ", soma)
  }
}
